<?xml version='1.0' encoding='utf-8'?>
<widget id="christianwilkinson.wordpress.com.tipcalculator" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>TipCalculator</name>
    <description>Simple Tip Calculator</description>
    <author email="christian.wilkinson@bobcats.gcsu.edu" href="christianwilkinson.wordpress.com">Christian Wilkinson</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="default" />
</widget>
